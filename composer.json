{
    "name": "yokai/security-token-bundle",
    "description": "Handle security token manipulation in a Symfony application",
    "license": "MIT",
    "authors": [
        {
            "name": "Yann Eugon√©",
            "email": "eugone.yann@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-openssl": "*",
        "symfony/framework-bundle": "~3.4|~4.0",
        "doctrine/orm": "^2.6",
        "doctrine/doctrine-bundle": "^1.9",
        "yokai/dependency-injection": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/yaml": "^4.1"
    },
    "autoload": {
        "psr-4": { "Yokai\\SecurityTokenBundle\\": "" }
    },
    "autoload-dev": {
        "files": [
            "Tests/app/YokaiSecurityTokenTestKernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
